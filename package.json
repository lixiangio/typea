{
   "name": "typea",
   "version": "8.3.2",
   "type": "module",
   "description": "JS 数据结构验证、转换器",
   "main": "./main/index.js",
   "exports": {
      ".": "./main/index.js",
      "./utility": "./main/utility.js",
      "./*": "./type/*"
   },
   "types": "./main/index.d.ts",
   "author": "lixiang",
   "license": "MIT",
   "scripts": {
