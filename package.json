{
   "name": "typea",
   "version": "7.0.0",
   "type": "module",
   "description": "JS数据结构验证、转换器",
   "main": "./build/index.js",
   "exports": {
      ".": "./dist/index.js",
      "./*": "./dist/*",
      "./types/*": "./types/*"
   },
   "types": "./types.d.ts",
   "author": "lixiang",
   "license": "MIT",
   "scripts": {
      "start": "tsc && node ./dist/index.js",
      "dev": "tsc --watch",
      "build": "tsc",
      "swc": "swc ./src/* -d ./dist && node ./dist/index.js",
      "test": "jtm",
      "jtm": "jtm ./test/common.js"
   },
   "keywords": [
      "typea",
      "schema",
      "validator"
   ],
   "repository": {
      "type": "git",
      "url": "git+https://github.com/xiangle/typea.git"
   },
   "bugs": {
      "url": "https://github.com/xiangle/typea/issues"
   },
   "homepage": "https://github.com/xiangle/typea#readme",
   "files": [
      "dist",
      "types"
   ],
   "devDependencies": {
      "jtm": "^1.0.4"
   },
   "directories": {
      "test": "test"
   }
}